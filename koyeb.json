{
  "name": "api",
  "type": "WEB",
  "routes": [
    {
      "port": 8080,
      "path": "/"
    }
  ],
  "ports": [
    {
      "port": 8080,
      "protocol": "http"
    }
  ],
  "env": [
    {
      "key": "JWT_SECRET",
      "value": "ovl_md_key"
    },
    {
      "key": "SMTP_USER",
      "value": "nignanfatao12345@gmail.com"
    },
    {
      "key": "SMTP_PASSWORD",
      "value": "famillenignan1@"
    },
    {
      "key": "USE_RECAPTCHA",
      "value": "false"
    }
  ],
  "regions": ["fra"],
  "scalings": [
    {
      "scopes": ["region:fra"],
      "min": 1,
      "max": 1,
      "targets": []
    }
  ],
  "instance_types": [
    {
      "scopes": ["region:fra"],
      "type": "free"
    }
  ],
  "health_checks": [],
  "skip_cache": true,
  "git": {
    "repository": "https://github.com/Nignanfatao/api",
    "branch": "koyeb",
    "build_command": "npm ci",
    "run_command": "npm start",
    "no_deploy_on_push": false
  },
  "domain": {
    "name": "api-ovl-md.koyeb.app",
    "custom_domain": true
  }
}
